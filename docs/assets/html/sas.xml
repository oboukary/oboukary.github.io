    <contexts>
      <context name="Normal" attribute="Normal" lineEndContext="#stay">
        <!-- Block comments -->
        <Detect2Chars char="/" char1="*" attribute="Comment" context="Comment"/>
        <!-- Line comments starting with * -->
        <RegExpr String="^\s*\*[^;]*;" attribute="Comment" context="#stay"/>
        
        <!-- Macro variables: &var. -> & is normal, var. is green -->
        <RegExpr String="&amp;+(?=[a-zA-Z_][a-zA-Z0-9_]*\.)" attribute="Normal" context="MacroVarResolved"/>
        <!-- Macro variables: &var (no dot) -> all normal/black -->
        <RegExpr String="&amp;+[a-zA-Z_][a-zA-Z0-9_]*" attribute="Normal" context="#stay"/>
        
        <!-- Strings -->
        <DetectChar char="&quot;" attribute="String" context="String"/>
        <DetectChar char="'" attribute="String" context="String2"/>
        
        <!-- DATA step: data followed by space and identifier (not data=) -->
        <RegExpr String="\bdata\s+(?=[a-zA-Z_])" attribute="BoldKeyword" context="#stay" insensitive="true"/>
        
        <!-- PROC FORMAT: format followed by space (not format=) -->
        <RegExpr String="\bformat\s+(?=[a-zA-Z_]|\;)" attribute="BoldKeyword" context="#stay" insensitive="true"/>
        
        <!-- Bold keywords: proc, run, quit, proc names -->
        <keyword String="boldkeywords" attribute="BoldKeyword" context="#stay"/>
        
        <!-- %macro and %mend - bold -->
        <RegExpr String="%(macro|mend)\b" attribute="BoldKeyword" context="#stay" insensitive="true"/>
        
        <!-- Macro keywords (%let, %if, etc.) - blue -->
        <keyword String="macrokeywords" attribute="Keyword" context="#stay"/>
        
        <!-- Regular keywords (includes data for data= option) -->
        <keyword String="keywords" attribute="Keyword" context="#stay"/>
        
        <!-- Functions -->
        <keyword String="functions" attribute="Keyword" context="#stay"/>
        
        <!-- User macro calls (%myMacro) - black bold italic -->
        <RegExpr String="%[a-zA-Z_][a-zA-Z0-9_]*" attribute="MacroCall" context="#stay"/>
        
        <!-- Numbers -->
        <Float attribute="Float" context="#stay"/>
        <Int attribute="DecVal" context="#stay"/>
      </context>
      
      <!-- Context for resolved macro variable (after &) -->
      <context name="MacroVarResolved" attribute="MacroVar" lineEndContext="#pop">
        <RegExpr String="[a-zA-Z_][a-zA-Z0-9_]*\." attribute="MacroVar" context="#pop"/>
      </context>
      
      <!-- Block comment -->
      <context name="Comment" attribute="Comment" lineEndContext="#stay">
        <Detect2Chars char="*" char1="/" attribute="Comment" context="#pop"/>
      </context>
      
      <!-- Double-quoted string with macro variable resolution -->
      <context name="String" attribute="String" lineEndContext="#stay">
        <RegExpr String="&amp;+(?=[a-zA-Z_][a-zA-Z0-9_]*\.)" attribute="String" context="MacroVarInString"/>
        <DetectChar char="&quot;" attribute="String" context="#pop"/>
      </context>
      
      <!-- Macro var inside string -->
      <context name="MacroVarInString" attribute="MacroVar" lineEndContext="#pop">
        <RegExpr String="[a-zA-Z_][a-zA-Z0-9_]*\." attribute="MacroVar" context="#pop"/>
      </context>
      
      <!-- Single-quoted string (no macro resolution) -->
      <context name="String2" attribute="String" lineEndContext="#stay">
        <DetectChar char="'" attribute="String" context="#pop"/>
      </context>
    </contexts>
    <itemDatas>
